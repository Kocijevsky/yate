match / {
    apply .foo | .foo.foo one

    apply .foo | .foo.foo two

    apply .foo | .foo.foo | .bar | .bar.bar three
}

match .foo one {
    ".foo@one { .id } "
}

match /.foo one {
    "/.foo@one { .id } "
}

match /.foo two {
    "/.foo@two { .id } "
}

match .foo two {
    ".foo@two { .id } "
}

match /.foo | .bar three {
    .id
}

